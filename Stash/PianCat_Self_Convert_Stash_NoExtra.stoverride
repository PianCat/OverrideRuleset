category: "Override"
sniffer: #!replace
  sniff:
    TLS:
      ports:
        - 443
        - 8443
    HTTP:
      ports:
        - 80
        - 8080
        - 8880
    QUIC:
      ports:
        - 443
        - 8443
  override-destination: false
  enable: true
  force-dns-mapping: true
  skip-domain:
    - Mijia Cloud
    - dlg.io.mi.com
    - +.push.apple.com
dns: #!replace
  enable: true
  ipv6: false
  prefer-h3: true
  enhanced-mode: redir-host
  default-nameserver:
    - 119.28.28.28
    - 119.29.29.29
    - 223.5.5.5
    - 223.6.6.6
  nameserver:
    - system
    - 119.29.29.29
    - 223.5.5.5
    - quic://223.5.5.5
    - tls://dot.pub:853
    - tls://dns.alidns.com:853
    - https://dot.pub:443/dns-query
    - https://dns.alidns.com:443/dns-query
  fallback:
    - quic://dns0.eu
    - https://dns.cloudflare.com/dns-query
    - https://dns.sb/dns-query
    - tcp://208.67.222.222
    - tcp://8.26.56.2
  proxy-server-nameserver:
    - quic://223.5.5.5
    - tls://dot.pub
